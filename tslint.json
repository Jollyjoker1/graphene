{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [
            true,
            "spaces",
            2
        ],
        "object-literal-sort-keys": false,
        "unified-signatures": false,
        "no-reference": false,
        "max-classes-per-file": false,
        "forin": false,
        "no-bitwise": false,
        "no-shadowed-variable": false,
        "no-namespace": false
    },
    "rulesDirectory": []
}